#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x9e7f20 .scope module, "jmb_mul2_tb" "jmb_mul2_tb" 2 10;
 .timescale 0 0;
v0x9fb400_0 .var "clock", 0 0;
v0x9fb4c0_0 .var "mult_1", 7 0;
v0x9fb570_0 .var "mult_2", 7 0;
v0x9fb620_0 .net "prod", 15 0, L_0x9fb700; 1 drivers
S_0x9e8010 .scope module, "u1" "jmb_mul2" 2 18, 3 15, S_0x9e7f20;
 .timescale 0 0;
v0x9c4f40_0 .net "mult_1", 7 0, v0x9fb4c0_0; 1 drivers
v0x9fb2c0_0 .net "mult_2", 7 0, v0x9fb570_0; 1 drivers
v0x9fb360_0 .alias "prod", 15 0, v0x9fb620_0;
L_0x9fb700 .arith/mult 16, v0x9fb4c0_0, v0x9fb570_0;
    .scope S_0x9e7f20;
T_0 ;
    %delay 5, 0;
    %load/v 8, v0x9fb400_0, 1;
    %inv 8, 1;
    %set/v v0x9fb400_0, 8, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x9e7f20;
T_1 ;
    %vpi_call 2 25 "$display", "Beginning simulation";
    %set/v v0x9fb400_0, 0, 1;
    %set/v v0x9fb4c0_0, 0, 8;
    %set/v v0x9fb570_0, 0, 8;
    %load/v 8, v0x9fb620_0, 16;
    %cmpi/u 8, 0, 16;
    %inv 4, 1;
    %jmp/0xz  T_1.0, 4;
    %vpi_call 2 30 "$display", "Multiplier failed";
T_1.0 ;
    %delay 5, 0;
    %movi 8, 2, 8;
    %set/v v0x9fb4c0_0, 8, 8;
    %load/v 8, v0x9fb620_0, 16;
    %cmpi/u 8, 0, 16;
    %inv 4, 1;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 35 "$display", "Multiplier failed";
T_1.2 ;
    %delay 10, 0;
    %movi 8, 3, 8;
    %set/v v0x9fb570_0, 8, 8;
    %load/v 8, v0x9fb620_0, 16;
    %cmpi/u 8, 6, 16;
    %inv 4, 1;
    %jmp/0xz  T_1.4, 4;
    %vpi_call 2 40 "$display", "Multiplier failed";
T_1.4 ;
    %delay 10, 0;
    %movi 8, 15, 8;
    %set/v v0x9fb4c0_0, 8, 8;
    %movi 8, 4, 8;
    %set/v v0x9fb570_0, 8, 8;
    %load/v 8, v0x9fb620_0, 16;
    %cmpi/u 8, 61, 16;
    %inv 4, 1;
    %jmp/0xz  T_1.6, 4;
    %vpi_call 2 46 "$display", "Multipier failed";
T_1.6 ;
    %vpi_call 2 48 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "jmb_mult2_tb.v";
    "./../jmb_mul2.v";
